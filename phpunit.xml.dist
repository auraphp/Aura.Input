<phpunit bootstrap="./phpunit.php">
    <testsuites>
        <testsuite name="Aura.Input test suite">
            <directory>./tests</directory>
        </testsuite>
    </testsuites>
    <filter>
      <whitelist processUncoveredFilesFromWhitelist="true">
        <directory suffix=".php">./src/</directory>
      </whitelist>
    </filter>
</phpunit>
